{
  "entrypoint": "index.html",
  "shell": "elements/sc-drawer-layout.html",
  "sources": [
    "elements/**/*",
    "img/**/*"
  ],
  "extraDependencies": [
    "localization/elements/**/*.json",
    "node_modules/workbox-sw/build/importScripts/*",
    "manifest.json",
    "files/**/*",
    "push_manifest.json",
    "service-worker.js",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js.map"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": false
      },
      "js": {
        "compile": true,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    },
    {
      "name": "default",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": false
      },
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}